@model DigitalPhotoPrintingEproject.Models.GelleryTable

@{
    ViewBag.Title = "Gellery";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Gellery</h2>
@Html.Raw(ViewBag.msg)
@using (Html.BeginForm("Gellery","Home", FormMethod.Post, new { enctype = "multipart/form-data" })) 
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
              <div class="form-group">
            @Html.LabelFor(model => model.mg_Image, htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    <input type="file" name="Imageayi" required />
                </div>
                @Html.ValidationMessageFor(model => model.mg_Image, "", new { @class = "text-danger" })
            </div>
       
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
