@model IEnumerable<DigitalPhotoPrintingEproject.Models.GelleryTable>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.mg_Image)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserTable.U_Name)
        </th>
        <th></th>
    </tr>
@foreach (var item in Model) {
    <tr>
        <td>
            <img src="@Url.Content(item.mg_Image)" alt="Alternate Text" width="100" height="100" />      
          </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserTable.U_Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.mg_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.mg_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.mg_Id })
        </td>
    </tr>
}

</table>
